{
  "name": "simple-tree-component",
  "version": "0.0.1",
  "description": "A simple to use ui-tree javascript component.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/code-chris/simple-tree-component.git"
  },
  "keywords": [
    "js",
    "tree",
    "component",
    "tree-component",
    "input",
    "ui"
  ],
  "author": "Christian Kotzbauer <christian.kotzbauer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/code-chris/simple-tree-component/issues"
  },
  "homepage": "https://github.com/code-chris/simple-tree-component#readme",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build": "run-s build:pre build:build build:types build:post",
    "build:pre": "rimraf dist",
    "build:build": "ts-node --transpile-only build.ts",
    "build:types": "tsc -p tsconfig.declarations.json",
    "build:post": "ts-node --transpile-only build-post.ts",
    "format": "prettier --ignore-path .gitignore --trailing-comma es5 --write \"**/*.ts\"",
    "lint": "eslint --ignore-path .gitignore \"**/*.ts\"",
    "start": "npm run build:build -- --dev",
    "test": "run-s test:typecheck test:unit",
    "test:typecheck": "tsc --noEmit",
    "test:unit": "jest --config config/jest.json",
    "codecov": "npm run test:unit -- --coverage && codecov -f ./coverage/lcov.info"
  },
  "devDependencies": {
    "@code-chris/changelog-generator": "1.0.0",
    "@rollup/plugin-typescript": "4.0.0",
    "@types/autoprefixer": "9.7.1",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.2",
    "@types/node-sass": "4.11.0",
    "@typescript-eslint/eslint-plugin": "2.24.0",
    "@typescript-eslint/parser": "2.24.0",
    "autoprefixer": "9.7.4",
    "chokidar": "3.3.1",
    "codecov": "3.6.5",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-prettier": "3.1.2",
    "jest": "25.1.0",
    "node-sass": "4.13.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "rimraf": "3.0.2",
    "rollup": "2.1.0",
    "terser": "4.6.7",
    "ts-jest": "25.2.1",
    "ts-node": "8.7.0",
    "tslib": "1.11.1",
    "typescript": "3.8.3"
  }
}
